  <Product textId="UCC 1Click Pay" processingProductId="11726">
    <InputControlType>numpad</InputControlType>
    <InputControlLocale>en-US</InputControlLocale>
    <InputFields>
      <Field name="customerId" length="8" fieldMask="^[0-9]{8}$" fieldFlags="9"  sortIndex="1" type="FormatHintedNumber" />
    </InputFields>
    <OutputFields>
      <Field name="customerId" hashKey="customerId" sortIndex="1" />
      <Field name="abonentName" hashKey="abonentName" sortIndex="2" />
     <Field name="balanceGEL" hashKey="abonentDebt" sortIndex="4" multiplier="-1" fieldFlags="9216" />
      <Field name="actualAmount" hashKey="actualAmount" sortIndex="6" fieldFlags="2" />
	   <Field name="commission" hashKey="commission" sortIndex="5" fieldFlags="1" />
    </OutputFields>
    <ProviderQueryFormatString>{0}</ProviderQueryFormatString>
    <PaymentInvoiceFormatString>{0}</PaymentInvoiceFormatString>
    <PaymentInvoiceFields>
      <Field name="customerId" sortIndex="1" />
    </PaymentInvoiceFields>
    <ProviderQueryFields>
      <Field name="customerId" sortIndex="1" />
    </ProviderQueryFields>
    <ValidationAmount>3</ValidationAmount>
    <Amount>
      <Commission>
        <IsProviderCommission>false</IsProviderCommission>
        <CommissionStage>
          <AmountLessThan>10000</AmountLessThan>
          <AmountGreaterThan>0</AmountGreaterThan>
          <Type>Fixed</Type>
          <Value>100</Value>
        </CommissionStage>
        <CommissionStage>
          <AmountLessThan>20000</AmountLessThan>
          <AmountGreaterThan>10001</AmountGreaterThan>
          <Type>Fixed</Type>
          <Value>200</Value>
        </CommissionStage>
        <CommissionStage>
          <AmountGreaterThan>20001</AmountGreaterThan>
          <Type>Fixed</Type>
          <Value>300</Value>
        </CommissionStage>
      </Commission>
      <Minimum>150</Minimum>
      <Maximum>100000</Maximum>
      <AmountBase>1</AmountBase>
      <AcceptedCash>
        <Cash id="1" />
        <Cash id="2" />
        <Cash id="3" />
        <Cash id="4" />
        <Cash id="5" />
        <Cash id="6" />
        <Cash id="7" />
        <Cash id="8" />
        <Cash id="13" />
        <Cash id="14" />
        <Cash id="15" />
      </AcceptedCash>
    </Amount>
    <OnExitTemplate>NavigateHome</OnExitTemplate>
    <PrinterName>FirstPrinter</PrinterName>
    <Devices>
      <Device>
        <Name>CashCode</Name>
      </Device>
      <Device>
        <Name>CoinNRI</Name>
      </Device>
      <Device>
        <Name>PrinterStar</Name>
      </Device>
    </Devices>
  </Product>
